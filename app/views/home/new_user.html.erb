<div id="bigimage">

</div>
<div id="middleAbout">
	<div id="header">
		<h1>Question your campus</h1>
		<span id="tag">
			The best place to find out about your campus
		</span>
	</div>
	<div id="footerbottom">
		<div id="footersignin">
			<p>
				<h4>Sign in to join our private beta with your facebook.</h4>
			</p>
			<%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook), :class=> "ir social" %>
		</div><!-- #footersignin -->
		<div id="footeregister">
			<p>
				<h4>If you would like your campus to have this service, fill out the form.</h4>
			</p>
			<p>
				<form>
				Campus : <input type="text" name="firstname" /><br />
				Email : <input type="text" name="lastname" />
				</form>
			</p>
		</div><!-- #footerregister -->
	</div><div class="clear"></div>
</div>


